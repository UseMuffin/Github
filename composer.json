{
    "name": "muffin/github",
    "description": "Github Webservice (API) for CakePHP 3.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "muffin",
        "webservice",
        "github",
        "api"
    ],
    "homepage": "https://github.com/usemuffin/github",
    "license": "MIT",
    "authors": [
        {
            "name": "Jad Bitar",
            "homepage": "http://jadb.io",
            "role": "Author"
        },
        {
            "name": "ADmad",
            "homepage": "https://github.com/ADmad",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/usemuffin/github/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/usemuffin/github/issues",
        "source": "https://github.com/usemuffin/github"
    },
    "require": {
        "muffin/webservice": "^1.0",
        "knplabs/github-api": "~1.4@dev"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0",
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-4": {
            "Muffin\\Github\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muffin\\Github\\Test\\": "tests"
        }
    }
}
